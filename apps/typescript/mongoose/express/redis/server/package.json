    "@types/helmet": "^0.0.47",
    "@types/mongodb": "^3.5.18",
    "@types/mongoose": "^5.7.20",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.1",
    "@types/redis": "^2.8.21",
    "nodemon": "^2.0.4",
    "poetic": "^1.5.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
